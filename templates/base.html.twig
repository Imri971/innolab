<!DOCTYPE html>
<html>

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">

  <script type="text/javascript">
    window.$crisp = [];
    window.CRISP_WEBSITE_ID = "7417f5bf-ec8a-4e29-a548-63063db489b2";
    (function () {
      d = document;
      s = d.createElement("script");
      s.src = "https://client.crisp.chat/l.js";
      s.async = 1;
      d.getElementsByTagName("head")[0].appendChild(s);
    })();
  </script>

  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.1/css/all.css">
  <link rel="stylesheet" href="{{ asset('css/media-queries.css') }}" />
  <link href="{{ asset('css/bootstrap.min.css') }}" type="text/css" rel="stylesheet">
  <link rel="icon" href="{{asset('build/images/innolabLamp.png')}}" />
  <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->

  <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->

  <!--[if lt IE 9]>

  <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>

  <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>

<![endif]-->
  <title>{% block title %}{% endblock %}</title>
  {% block stylesheets %}
  {{ encore_entry_link_tags ( 'app' ) }}
  {% endblock %}
</head>

<body>
  <header {% block header_class %}{% endblock %}>

    <nav class="navbar navbar-expand-lg navbar-light ">
      <a class="navbar-brand logo_Nav" href="{{ path('innolab')}}"><img src="{{asset('build/images/innolabLamp.png')}}"
          alt="Logo" /></a>

      <button class="navbar-toggler m-auto" type="button" data-toggle="collapse" data-target="#navbarNavDropdown"
        aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      {% if app.user %}
      <div class=" d-flex w-100">
        <p class="badge badge-pill bg-prus nav_user yellow mr-3">Bienvenue {{app.user.name}} </p><small
            class="badge badge-pill bg-light nav_user--admin text-dark">{% if is_granted('ROLE_ADMIN') %} ADMIN
            {% endif %}</small>

      </div>

      {% endif %}


      <div class="collapse navbar-collapse " id="navbarNavDropdown">
        <ul class="navbar-nav ml-auto mr-3">
          <li class="nav-item">
            <a class="nav-link" href="/">Accueil</a>
          </li>


          <li class="nav-item">
            <a class="nav-link" href="{{ path('program')}}">Programmes</a>
          </li>

          <li class="nav-item">
            <a class="nav-link" href="{{ path('contact')}}">Contact</a>
          </li>
        </ul>
      </div>

      {% if not app.user %}
      <div class="col-lg-1 navUser">
        <div class="  d-flex justify-content-center text-nowrap ml-auto">
          <a href="{{ path('app_login')}}" class="navUser__a mr-lg-3" title="login"><i class="fas fa-lock iconNav"></i> Se
            connecter</a>
        </div>
      </div>
      {% else %}
      <div class="col-lg-1 navUser">
        <div class="d-flex  justify-content-center ">

          <div class="d-flex justify-content-center align-items-center text-nowrap">
            <a href="{{ path('app_logout')}}" class=" navUser__a mr-lg-3" title="logout"><i
                class="fas fa-sign-in-alt iconNav"></i> Se déconnecter</a>
          </div>

        </div>
      </div>
      {% endif %}

    </nav>


    {% block banner %}{% endblock %}
  </header>

  {% block body %}
  {% endblock %}

  <footer class="d-flex align-items-center mt-4 {% block footerPosition %}{% endblock %}">
    <div class="container col-lg-10 mt-3">
      <div class="row text-left ">
        <div class="innolab_infos col-sm-6 col-md-6 col-lg-3">

          <div class="col-md-8 col-sm-8 innolab__infos--block">
            <h2>Innolab 62</h2>
            <ul class="foot-contact">
              <li class="text-nowrap"><i class="fas fa-map-marker-alt "></i> 16 place de la Préfecture, 62000 Arras</li>
              <li><i class="fas fa-mobile-alt"></i> 03 21 21 92 25</li>
              <li class="text-nowrap"><i class="fas fa-envelope"></i><a href="mailto:innolab62@pasdecalais.fr">
                  innolab62@pasdecalais.fr</a></li>
              <li><img src="{{asset('build/images/global.svg')}}" alt=""> <a href="https://www.pasdecalais.fr/"
                  target="_blank">pasdecalais.fr</a></li>

            </ul>
          </div>
        </div>

        <div class="col-sm-6 col-md-6 col-lg-3 ">
          <div class="text-left col-12 d-flex flex-column align-items-center">
            <h2>Informations</h2>
            <ul class="list-links ">
              <li><i class="far fa-caret-square-right mr-1"></i><a href="/#projets">Projets incubés</a></li>
              <li><i class="far fa-caret-square-right mr-1"></i><a href="{{path('program')}}">Programmes</a></li>
              <li><i class="far fa-caret-square-right mr-1"></i><a href="{{path('contact')}}">Contact</a></li>

            </ul>
          </div>
        </div>

        <div class="col-sm-6 col-md-6 col-lg-3 text-center">
          <h2>Réseaux </h2>
          <p>Suivez nous sur tous nos réseaux </p>
          <ul class="social">
            <li>
              <a href="https://twitter.com/innolab62?lang=fr" class="twitter" target="_blank"><i
                  class="fab fa-twitter"></i></a> </li>
            <li><a href="https://www.linkedin.com/in/laurent-jakubowski-01080627/" class="linkedin" target="_blank"><i
                  class="fab fa-linkedin-in"></i></a> </li>
            <li><a href="https://github.com/betagouv/blog.beta.gouv.fr/blob/master/content/_incubators/innolab-62.md"
                class="github" target="_blank"> <i class="fab fa-github"></i></a> </li>

          </ul>
        </div>
        <div class="col-sm-6 col-md-6 col-lg-3 mt-3">
          <ul>
            <li> <a href="#" class="betagouv"> <img src="{{asset('build/images/betagouvfr.svg')}}" alt=""></a></li>
            <li><a href="#" class="pasdecalais"> <img src="{{asset('build/images/pasdecalais.png')}}" class="mt-2"
                  alt=""></a></li>

          </ul>


        </div>


      </div>
    </div>
  </footer>

  <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js"
    integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous">
  </script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"
    integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous">
  </script>
  <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"
    integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous">
  </script>

 
  {% block javascripts %}
  {{ encore_entry_script_tags ( 'app' ) }}
  {% endblock %}

  <script src="{{asset('js/main.js')}}"></script>
</body>

</html>